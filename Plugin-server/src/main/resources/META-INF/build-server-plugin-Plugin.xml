<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-autowire="constructor">
    <bean class="TeamCity.UI.Tabs.DeployTab"/>
    <bean class="TeamCity.controllers.DeployControllerAction"/>
    <bean class="TeamCity.UI.Tabs.SettingsAdminPage"/>
    <bean class="TeamCity.controllers.SettingAdminController"/>
    <bean class="TeamCity.controllers.MessageListener"/>
    <bean class="TeamCity.controllers.DbAccessTest"/>
    <bean class="TeamCity.powershell.PowerShellFactory"/>
</beans>
